{
  "extends": "ts-node/node14/tsconfig.json",
  "ts-node": {
    "transpileOnly": false,
    "files": true,
    "compilerOptions": {}
  },
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "target": "ESNext",
    "lib": ["ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "types": ["node"],
    "outDir": "dist/out-tsc",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "module": "ESNext"
  },
  "include": ["**/*"],
  "files": ["src/index.ts"],
  "exclude": ["dist"]
}
